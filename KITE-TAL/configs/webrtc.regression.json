{
  "name": "webrtc-1v1-媒体服务改动上线回归测试",
  "callable": true,
  "grids": [
    {
      "type": "local",
      "url": "http://localhost:4444/wd/hub"
    }
  ],
  "tests": [
    {
      "name": "1v1 for different server %ts",
      "tupleSize": 1,
      "description": "跨服务器 1v1 推拉流互通",
      "testImpl": "org.tal.webrtc.tests.WebRtc1v1Test",
      "csvReport": true,
      "payload": {
        "url": "https://8-140-113-148.eaydu.com/index",
        "localServer": "8-140-113-148.eaydu.com",
        "remoteServer": "112-126-100-194.eaydu.com",
        "takeScreenshotForEachTest": true,
        "getStats": {
          "enabled": false,
          "statsCollectionTime": 2000,
          "statsCollectionInterval": 2000,
          "peerConnections": [
            "stream.transport"
          ],
          "selectedStats": [
            "inbound-rtp",
            "outbound-rtp",
            "candidate-pair",
            "track"
          ]
        }
      }
    },
    {
      "name": "1v1 for simple server %ts",
